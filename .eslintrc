{
  "root": true,
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "babelOptions": {
      "configFile": false
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "@react-native-community"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "usePrettierrc": true,
        "endOfLine": "auto"
      }
    ]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "GeoTIFF": "readonly"
  }
}